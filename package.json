{
  "name": "smartflow-web",
  "version": "0.1.1",
  "description": "Web application showcasing the benefits of using Smartflow",
  "main": "server.js",
  "private": true,
  "scripts": {
    "build": "webpack",
    "babel": "babel src -d dist",
    "serve": "webpack-dev-server --progress --colors  --content-base dist/ --watch",
    "watch2": "webpack --watch --devtool source-map ./src/app.js",
    "watch": "webpack --watch ./src/app.js ./dist/bundle.js",
    "start2": "webpack-dev-server --content-base dist/ ./src/app.js --watch",
    "postinstall": "webpack-dev-server --config ./webpack.prod.config.js --progress --colors  --content-base dist/ --watch",
    "start": "server",
    "prod": "NODE_ENV=production node server.js"
  },
  "watch": {
    "test": "{src}/*.js"
  },
  "dependencies": {
    "babel-cli": "6.26.0",
    "babel-jest": "^21.0.2",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "express": "^4.16.2",
    "smartflow": "^0.1.6",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7"
  },
  "devDependencies": {
    "concat": "^1.0.3",
    "css-loader": "^0.28.7",
    "eslint": "^3.19.0",
    "eslint-config-google": "^0.8.0",
    "jest": "^21.1.0",
    "jest-cli": "^21.1.0",
    "regenerator-runtime": "^0.11.1",
    "style-loader": "^0.19.0"
  }
}
